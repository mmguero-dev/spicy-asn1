module ASN;

import ASN1;
import spicy;

#-----------------------------------------------------------------------------
public type Messages = unit {
    : Message[];
};

#-----------------------------------------------------------------------------
public type Message = unit {
    var success: bool = False;
    _payload : bytes &eod;

    : ASN1::ASN1Message(True) {
        self.success = True;
    } &parse-from=self._payload;

};